{
    "name": "tomas-votruba/symfony-upgrade-demo-2022",
    "require": {
        "php": "^7.2",
        "symfony/forms": "^2.8"
    },
    "require-dev": {
        "symplify/config-transformer": "^11.1",
        "symplify/vendor-patches": "^11.1",
