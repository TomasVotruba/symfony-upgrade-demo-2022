{
    "name": "tomas-votruba/symfony-upgrade-demo-2022",
    "require": {
        "php": "^7.2",
        "symfony/form": "^2.8",
        "symfony/event-dispatcher": "^2.8",
        "doctrine/orm": "^2.7"
    },
    "require-dev": {
        "symplify/config-transformer": "^11.1",
        "symplify/vendor-patches": "^11.1",
        "symplify/phpstan-rules": "^11.1",
        "rector/rector": "^0.14.8",
        "phpstan/phpstan": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "config": {
        "allow-plugins": {
            "cweagans/composer-patches": true
        }
    },
    "scripts": {
        "demo-transform-config": "vendor/bin/config-transformer switch-format config --dry-run --ansi",
        "demo-transform-config-no-dry": "vendor/bin/config-transformer switch-format config --ansi",
        "demo-generate-patch": "vendor/bin/vendor-patches generate",
        "demo-mixed-caller-fetcher": "vendor/bin/phpstan analyze --configuration phpstan-mixed-caller-fetcher.neon",
        "demo-type-coverage": "vendor/bin/phpstan analyze --configuration phpstan-type-coverage.neon",
        "demo-rename-class": "vendor/bin/rector process --config rector-rename-class.php --dry-run",
        "demo-rename-method": "vendor/bin/rector process --config rector-rename-method.php --dry-run",
        "demo-doctrine-attributes": "vendor/bin/rector process --config rector-attributes.php --dry-run",
        "demo-nested-attributes": "vendor/bin/rector process --config rector-nested-attributes.php --dry-run"
    }
}
